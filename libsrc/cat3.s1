* cat3.s1 -- CAT version for substring capture, CRB, Mar 29, 2014

  BEGIN CAT3;
  ENTRY CAT3;

  DCL I,J,LEN;

* Append substring from BUFF into DEST
PROC CAT3(DEST,LA,SRC);
  LEN=LA SHR 8;
  I=LA AND 255;                 * anchor index in SRC
  J=DEST;
  DEST=DEST+LEN;
  DO WHILE LEN;
    DEST(J)=SRC(I);
    I=I+1;
    J=J+1;
    LEN=LEN-1;
    ENDDO
  RETURN
  ENDPROC

END
