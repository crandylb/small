* TESTCAT.S1 -- Test CAT2 in Small, CRB, Dec 18, 2013

  ENTRY TESTCAT;
  BEGIN TESTCAT;
  START TESTCAT;

  EXT PROC CAT2,WRITE;

  DCL OUTCH=3;
  MSG HELLO='Hello';
  DCL PAD=(15*' ');
  MSG SMALLW=' Small World.';

LABEL TESTCAT;
  CALL WRITE(OUTCH,HELLO);
  CALL WRITE(OUTCH,SMALLW);
  CALL CAT2(HELLO,SMALLW);
  CALL WRITE(OUTCH,HELLO);
  RETURN
END
